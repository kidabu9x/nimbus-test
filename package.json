{
  "name": "nimbus-test",
  "version": "1.0.0",
  "description": "Nimbus test built with the MEVN stack",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "test": "echo \"Error: no test specified\" && exit 1",
    "server": "nodemon server.js",
